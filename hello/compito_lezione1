#include <stdio.h>
int main()
{
    char name[32];
    char surname[32];
    
    printf("Enter your name:\n");
    scanf("%s", name);
    printf("Enter your surname:\n");
    scanf("%s", surname);

    printf("\nBenvenuto %s", name);
    printf(" %s", surname);
    printf("\nGrazie per aver usato il nostro programma!");
    printf("\nMa sopratutto buone feste!!\n\n\n");
    
    return 0;
}